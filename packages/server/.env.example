# Server environment configuration
# Copy this file to .env and fill in the values

# Base chain RPC URL (defaults to mainnet.base.org)
BASE_RPC_URL=https://mainnet.base.org

# Ethereum private key for server-side transactions (REQUIRED)
# DO NOT use a key with real funds for development
PRIVATE_KEY=0x

# Server port (optional)
PORT=3069

# Pinata API Keys for IPFS upload (REQUIRED for tests)
PINATA_API_KEY=
PINATA_SECRET_API_KEY=

# OpenAI API Key for coin metadata generation (REQUIRED for tests)
OPENAI_API_KEY=